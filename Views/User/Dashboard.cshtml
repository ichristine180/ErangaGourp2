@model IEnumerable<E_ranga.Models.UserRegister>
@{
    ViewData["Title"] = "Admin Dashboard";
}
<h2>Users</h2>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>FirstName</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.FirstName</td>
                <td>@user.Email</td>
            </tr>
        }
    </tbody>
</table>
